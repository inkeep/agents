{
  "name": "@inkeep/agents-sdk",
  "version": "0.47.2",
  "description": "Agents SDK for building and managing agents in the Inkeep Agent Framework",
  "types": "dist/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "knip": "knip --directory ../.. --workspace packages/agents-sdk --config packages/agents-sdk/knip.config.ts --dependencies",
    "build": "tsdown",
    "dev": "pnpm build --watch",
    "test": "ENVIRONMENT=test vitest --run",
    "test:watch": "ENVIRONMENT=test vitest",
    "test:coverage": "ENVIRONMENT=test vitest --run --coverage",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "lint": "biome lint --error-on-warnings",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "dependencies": {
    "@inkeep/agents-core": "workspace:^",
    "@yarnpkg/lockfile": "^1.1.0",
    "simplematter": "^1.0.0",
    "js-yaml": "^4.1.0",
    "typescript": "^5.3.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.19.3",
    "@types/yarnpkg__lockfile": "^1.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.11.0",
  "engines": {
    "node": ">=22.18.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE.md",
    "SUPPLEMENTAL_TERMS.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkeep/agents.git",
    "directory": "packages/agents-sdk"
  },
  "keywords": [
    "agents",
    "ai",
    "framework",
    "sdk",
    "inkeep"
  ],
  "author": "Inkeep",
  "license": "SEE LICENSE IN LICENSE.md"
}
