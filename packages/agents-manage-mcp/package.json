{
  "name": "@inkeep/agents-manage-mcp",
  "version": "0.38.1",
  "author": "Speakeasy",
  "type": "module",
  "sideEffects": false,
  "bin": {
    "@inkeep/agents-manage-mcp": "bin/mcp-server.js",
    "mcp": "bin/mcp-server.js"
  },
  "scripts": {
    "build": "bun i && bun src/mcp-server/build.mts && tsup",
    "check": "npm run lint",
    "lint": "eslint --cache --max-warnings=0 src",
    "prepublishOnly": "npm run build",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "fetch-openapi": "node scripts/fetch-openapi.mjs",
    "generate": "node scripts/generate.mjs",
    "watch": "node scripts/watch-and-fetch.mjs"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@stricli/core": "^1.1.2",
    "bun": "^1.2.12",
    "express": "^5.1.0"
  },
  "devDependencies": {
    "@anthropic-ai/mcpb": "^1.2.0",
    "@eslint/js": "^9.26.0",
    "@types/bun": "^1.2.13",
    "@types/express": "^5.0.1",
    "@types/node": "^22.15.20",
    "dotenv": "^16.4.7",
    "eslint": "^9.26.0",
    "find-up": "^7.0.0",
    "globals": "^16.0.0",
    "tsup": "^8.5.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.31.1",
    "zod": "^3.25.7"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inkeep/agents.git",
    "directory": "packages/agents-manage-mcp"
  }
}
