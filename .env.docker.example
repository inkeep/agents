############################################################
# REQUIRED ENVIRONMENT VARIABLES
############################################################

# Change to "development" if deploying to dev
ENVIRONMENT=production

# (1) AI Provider Keys (you need at least one)
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GOOGLE_GENERATIVE_AI_API_KEY=
AZURE_API_KEY=

# (2) From Nango dashboard at http://<vm_ip>:3050
NANGO_SECRET_KEY=

# (3) From SigNoz dashboard at http://<vm_ip>:3080
SIGNOZ_API_KEY=

# (4) Set these for the Manage UI at http://<vm_ip>:3000
PUBLIC_INKEEP_AGENTS_API_URL=http://<vm_ip>:3002
PUBLIC_NANGO_SERVER_URL=http://<vm_ip>:3050
PUBLIC_NANGO_CONNECT_BASE_URL=http://<vm_ip>:3051
PUBLIC_SIGNOZ_URL=http://<vm_ip>:3080

# (5) Set these for Agents API
INKEEP_AGENTS_MANAGE_UI_URL=http://<vm_ip>:3000
INKEEP_AGENTS_API_URL=http://<vm_ip>:3002
INKEEP_AGENTS_RUN_API_BYPASS_SECRET=<REPLACE_WITH_INKEEP_AGENTS_RUN_API_BYPASS_SECRET>

# Credentials for the initial admin user created during setup
INKEEP_AGENTS_MANAGE_UI_USERNAME=admin@example.com
INKEEP_AGENTS_MANAGE_UI_PASSWORD=<REPLACE_WITH_INKEEP_AGENTS_MANAGE_UI_PASSWORD>

# Generated with: openssl rand -base64 32
INKEEP_AGENTS_JWT_SIGNING_SECRET=<REPLACE_WITH_INKEEP_AGENTS_JWT_SIGNING_SECRET>
BETTER_AUTH_SECRET=<REPLACE_WITH_BETTER_AUTH_SECRET>
SPICEDB_PRESHARED_KEY=<REPLACE_WITH_SPICEDB_PRESHARED_KEY>

INKEEP_AGENTS_TEMP_JWT_PRIVATE_KEY=<REPLACE_WITH_INKEEP_AGENTS_TEMP_JWT_PRIVATE_KEY>
INKEEP_AGENTS_TEMP_JWT_PUBLIC_KEY=<REPLACE_WITH_INKEEP_AGENTS_TEMP_JWT_PUBLIC_KEY>

############################################################
# OPTIONAL: SLACK APP CONFIGURATION
############################################################
# Enable Work Apps section in the dashboard (Slack, etc.)
# NEXT_PUBLIC_ENABLE_WORK_APPS=true

# Get credentials from: https://api.slack.com/apps → Your App → Basic Information
# SLACK_CLIENT_ID=
# SLACK_CLIENT_SECRET=
# SLACK_SIGNING_SECRET=
# Your public URL for OAuth redirect
# SLACK_APP_URL=https://your-app.example.com


############################################################
# OPTIONAL: BLOB STORAGE CONFIGURATION
############################################################
# Used for persisting images and media from conversations.
# Provider selection order:
# 1) S3 when BLOB_STORAGE_S3_BUCKET is set
#    May use any S3 compatible storage (AWS S3, Cloudflare R2, Google Cloud, Backblaze B2, DigitalOcean Spaces, etc.)
# 2) Vercel Blob when BLOB_READ_WRITE_TOKEN is set
# 3) Local filesystem fallback when neither of the above are set. Set BLOB_STORAGE_LOCAL_PATH to override default path for this directory.
# ===== Local filesystem configuration =====
BLOB_STORAGE_LOCAL_PATH=.blob-storage
# ===== Vercel Blob configuration =====
# BLOB_READ_WRITE_TOKEN=
# ===== S3 configuration =====
# Required for AWS S3:
# BLOB_STORAGE_S3_BUCKET=
# BLOB_STORAGE_S3_REGION=
# BLOB_STORAGE_S3_ACCESS_KEY_ID=
# BLOB_STORAGE_S3_SECRET_ACCESS_KEY=
# Optional for AWS S3 (may be required for S3-compatible providers):
# Custom endpoint URL. Often required for non-AWS providers (R2, MinIO, B2, Spaces, etc.).
# BLOB_STORAGE_S3_ENDPOINT=
# Path-style URLs. Default false for AWS S3. Some S3-compatible providers require true.
# BLOB_STORAGE_S3_FORCE_PATH_STYLE=false
